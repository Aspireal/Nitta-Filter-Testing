<odoo>
    <!-- Sequences for RFQ -->
    <record id="seq_purchase_rfq" model="ir.sequence">
        <field name="name">Requests for Quotation</field>
        <field name="code">purchase.rfq</field>
        <field name="prefix">NITTA/RFQ</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
     <record id="purchase.seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order</field>
            <field name="prefix">NITTA/PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

</odoo>
